        rev = "882c65c0f16dcf25242676260f5db30ae3184e4f";
        hash = "sha256-vuhGuTENu1lMGLgFy/wMD+TBpvso0O/PKdNESLfqN+8=";
      };
      substitutions = [
        {
          path = "CMakeLists.txt";
          from = "VCS_URL https://github.com/gazebosim/\${GITHUB_NAME}.git";
          to = "VCS_TYPE path VCS_URL ${sources."gz_rendering_vendor/gz-rendering"}";
        }
      ];
    };
    "gz_rendering_vendor/gz-rendering" = substituteSource {
      src = fetchgit {
        name = "gz-rendering-source";
        url = "https://github.com/gazebosim/gz-rendering.git";
        rev = "d0b75c3a7d0bb9ff80bc4069fab13fc4501af008";
